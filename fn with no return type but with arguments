#include <stdio.h>
#include <stdlib.h>
 void isPalindrome(int num)
 {
     int rnum=0,r,n;
    n=num;
    while(n>0)
    {
        r=n%10;
        rnum=rnum*10+r;
        n=n/10;
    }
    if(rnum==num)
        printf("It is a palindrome");
    else
        printf("it is not a palindrome");
}

 int main()
{
    isPalindrome(151);
}
