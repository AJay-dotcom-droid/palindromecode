#include <stdio.h>
#include <stdlib.h>
 void isPalindrome()
 {
     int number,result;
     printf("Enter the number:");
    scanf("%d",&number);
     int rnum=0,r,n;
    n=number;
    while(n>0)
    {
        r=n%10;
        rnum=rnum*10+r;
        n=n/10;
    }
    if(rnum==number)
        printf("It is a palindrome");
    else
        printf("it is not a palindrome");
}

 int main()
{
    isPalindrome();
}
